<%- include('../partials/header'); %>
<main>
    <div class="container">
        <div class="row">
            <div class="col-12">
                <a href="<%= prefix %>/notification/create">Maak melding aan</a>
                <% notifications.forEach(function(notification) { %>
                    <div class="card">
                        <div class="card-header"></div>
                        <div class="card-body">
                            <%= notification.who %>
                            <%= notification.what %>
                            <%= notification.where %>
                        </div>
                    </div>
                <% }); %>
            </div>
        </div>
    </div>
</main>
<%- include('../partials/footer'); %>